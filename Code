function printidentity()
	print("Current identity is 8")
end

-- Create a RemoteEvent in ReplicatedStorage named "SendNotification"
local ReplicatedStorage = game:GetService("ReplicatedStorage")
local sendNotificationEvent = Instance.new("RemoteEvent")
sendNotificationEvent.Name = "SendNotification"
sendNotificationEvent.Parent = ReplicatedStorage

-- Server-side script
function createBlackHole()
    for _, part in pairs(workspace:GetDescendants()) do
        if part:IsA("BasePart") and not part.Anchored then
            part.CFrame = CFrame.new(0, 5, 0)
        end
    end
	local message = "Successfully loaded blackhole"
	local player = game:GetService("Players").LocalPlayer
	player:Notify({
		Title = "TuerScript";
		Text = message;
		Duration = 5;
	})
end

function ctrlclicktp()
	loadstring(game:HttpGet("https://cdn.wearedevs.net/scripts/Click%20Teleport.txt"))()
	local message = "Successfully loaded Ctrl + Click TP"
	local player = game:GetService("Players").LocalPlayer
	player:Notify({
		Title = "TuerScript";
		Text = message;
		Duration = 5;
	})
end

